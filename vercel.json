{
"version":1,
"builds":[
    {
        "src":"project/wsgi.py",
        "use":"@vercel/python",
        "config":{
            "maxLambdaSize":"15mb","runtime":"python3.10"
        }
    },
    {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"static_files_build"
        }
    }
],
"routes":[
    {
        "src":"/static/(.*)",
        "dest":"/static/$1"
    },
    {
        "src":"/(.*)",
        "dest":"project/wsgi.py"
    }
]
}